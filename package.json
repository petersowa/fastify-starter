{
	"name": "fastify-starter",
	"version": "0.1.0",
	"description": "Backend API Fastify server with Typescript support.",
	"main": "dist/app.js",
	"engines": {
		"node": "13.9.0",
		"npm": "6.14.1"
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "npm run copy-static-assets && concurrently \"npm run watch-ts\" \"nodemon --ignore data/ --ignore public/ dist/app.js\" \"npm run dev:client\"",
		"start": "npm run serve",
		"serve": "node dist/app.js",
		"watch-node": "nodemon --ignore data/ dist/app.js",
		"watch-ts": "tsc -w",
		"build": "npm run build:server && npm run build:client",
		"build:server": "npm run build-ts && npm run copy-static-assets",
		"build:client": "cd src/client/ && npm install && npm run build",
		"build-ts": "tsc",
		"lint": "eslint . --ext .ts --ext .js",
		"copy-static-assets": "ts-node copyStaticAssets.ts",
		"dev:client": "npm run dev --prefix ./src/client/"
	},
	"keywords": [
		"fastify",
		"starter",
		"node",
		"http-server"
	],
	"author": "Peter Sowa",
	"license": "ISC",
	"dependencies": {
		"bcrypt": "^4.0.1",
		"connect-mongo": "^3.2.0",
		"dotenv": "^8.2.0",
		"fastify": "^2.13.1",
		"fastify-cookie": "^3.6.0",
		"fastify-csrf": "^1.0.3",
		"fastify-flash": "^1.0.6",
		"fastify-formbody": "^3.1.0",
		"fastify-helmet": "^3.0.2",
		"fastify-plugin": "^1.6.1",
		"fastify-session": "^3.3.0",
		"fastify-static": "^2.6.0",
		"handlebars": "^4.7.6",
		"mongoose": "^5.9.9",
		"point-of-view": "^3.8.0",
		"socket.io": "^2.3.0"
	},
	"devDependencies": {
		"@types/bcrypt": "^3.0.0",
		"@types/connect-mongo": "^3.1.3",
		"@types/handlebars": "^4.1.0",
		"@types/mongoose": "^5.7.10",
		"@types/node": "^13.11.1",
		"@types/shelljs": "^0.8.7",
		"@types/socket.io": "^2.1.4",
		"@types/socket.io-client": "^1.4.32",
		"@typescript-eslint/eslint-plugin": "^2.28.0",
		"@typescript-eslint/parser": "^2.28.0",
		"babel-eslint": "^10.1.0",
		"concurrently": "^5.1.0",
		"eslint": "^6.8.0",
		"eslint-plugin-eslint-plugin": "^2.2.1",
		"eslint-plugin-prettier": "^3.1.3",
		"mocha": "^7.1.1",
		"shelljs": "^0.8.3",
		"ts-loader": "^6.2.2",
		"ts-node": "^8.8.2",
		"typescript": "^3.8.3",
		"webpack": "^4.42.1",
		"webpack-cli": "^3.3.11"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/petersowa/fastify-starter.git"
	},
	"bugs": {
		"url": "https://github.com/petersowa/fastify-starter/issues"
	},
	"homepage": "https://github.com/petersowa/fastify-starter#readme"
}
